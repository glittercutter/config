!---!! urxvt !!---!

urxvt*termName: rxvt
urxvt*internalBorder: 0
urxvt*background: Black
urxvt*scrollBar: false
urxvt*saveLines: 2000
urxvt.urgentOnBell: True	#false alarm on unfocus

#urxvt*fade: 20
urxvt*transparent: false
#urxvt*shading: 50

urxvt*matcher.button: 1					/* match left-mouse-button */
urxvt*urlLauncher: opera               	/* launch web browser on matched text */

urxvt*background: #030303
urxvt*foreground: #cfcfcf

